#! /cs/local/bin/vvp
:ivl_version "10.3 (stable)" "(v10_3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xc06e00 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0xc06f80_0 .var "instruction", 31 0;
    .scope S_0xc06e00;
T_0 ;
    %vpi_call 2 5 "$display", "Time:", $time {0 0 0};
    %vpi_call 2 6 "$display", "Instruction: ", v0xc06f80_0 {0 0 0};
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0xc06f80_0, 0, 32;
    %vpi_call 2 8 "$display", "Time:", $time {0 0 0};
    %vpi_call 2 9 "$display", "Instruction: ", v0xc06f80_0 {0 0 0};
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0xc06f80_0, 0, 32;
    %vpi_call 2 11 "$display", "Time:", $time {0 0 0};
    %vpi_call 2 12 "$display", "Instruction: ", v0xc06f80_0 {0 0 0};
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0xc06f80_0, 0, 32;
    %vpi_call 2 14 "$display", "Time:", $time {0 0 0};
    %vpi_call 2 15 "$display", "Instruction: ", v0xc06f80_0 {0 0 0};
    %vpi_call 2 16 "$finish" {0 0 0};
    %end;
    .thread T_0;
